{
  "id": "customer_journey",
  "title": "고객 획득 여정",
  "category": "business",
  "keywords": ["고객", "여정", "획득", "마케팅", "판매", "구매", "전환"],
  "description": "잠재 고객부터 충성 고객까지의 전환 여정을 보여주는 프로세스 흐름도",
  "color_palette": {
    "primary": "#3C40C6",
    "secondary": "#FF4757",
    "accent": "#FFA502",
    "background": "#0A0A0A"
  },
  "svg_template": `
    <div class="diagram-container">
      <div class="diagram-title">{title} 핵심 프로세스</div>
      <svg width="100%" height="350" viewBox="0 0 800 350">
        <!-- 단계별 고객 여정 -->
        <rect x="50" y="80" width="100" height="60" rx="8" fill="#3C40C6"/>
        <text x="100" y="105" text-anchor="middle" fill="white" font-size="11" font-weight="bold">인식</text>
        <text x="100" y="120" text-anchor="middle" fill="white" font-size="10">(Awareness)</text>

        <rect x="180" y="80" width="100" height="60" rx="8" fill="#5F27CD"/>
        <text x="230" y="105" text-anchor="middle" fill="white" font-size="11" font-weight="bold">관심</text>
        <text x="230" y="120" text-anchor="middle" fill="white" font-size="10">(Interest)</text>

        <rect x="310" y="80" width="100" height="60" rx="8" fill="#FF4757"/>
        <text x="360" y="105" text-anchor="middle" fill="white" font-size="11" font-weight="bold">고려</text>
        <text x="360" y="120" text-anchor="middle" fill="white" font-size="10">(Consideration)</text>

        <rect x="440" y="80" width="100" height="60" rx="8" fill="#FFA502"/>
        <text x="490" y="105" text-anchor="middle" fill="white" font-size="11" font-weight="bold">구매</text>
        <text x="490" y="120" text-anchor="middle" fill="white" font-size="10">(Purchase)</text>

        <rect x="570" y="80" width="100" height="60" rx="8" fill="#2ED573"/>
        <text x="620" y="105" text-anchor="middle" fill="white" font-size="11" font-weight="bold">충성</text>
        <text x="620" y="120" text-anchor="middle" fill="white" font-size="10">(Loyalty)</text>

        <!-- 각 단계별 전략 -->
        <rect x="50" y="200" width="100" height="40" rx="6" fill="#E17055"/>
        <text x="100" y="220" text-anchor="middle" fill="white" font-size="9">콘텐츠 마케팅</text>

        <rect x="180" y="200" width="100" height="40" rx="6" fill="#A29BFE"/>
        <text x="230" y="220" text-anchor="middle" fill="white" font-size="9">리드 마그네트</text>

        <rect x="310" y="200" width="100" height="40" rx="6" fill="#FD79A8"/>
        <text x="360" y="220" text-anchor="middle" fill="white" font-size="9">개인화 제안</text>

        <rect x="440" y="200" width="100" height="40" rx="6" fill="#FDCB6E"/>
        <text x="490" y="220" text-anchor="middle" fill="white" font-size="9">결제 간소화</text>

        <rect x="570" y="200" width="100" height="40" rx="6" fill="#6C5CE7"/>
        <text x="620" y="220" text-anchor="middle" fill="white" font-size="9">리워드 시스템</text>

        <!-- 진행 화살표 -->
        <path d="M150 110 L180 110" stroke="#FF4757" stroke-width="3" marker-end="url(#arrow3)"/>
        <path d="M280 110 L310 110" stroke="#FF4757" stroke-width="3" marker-end="url(#arrow3)"/>
        <path d="M410 110 L440 110" stroke="#FF4757" stroke-width="3" marker-end="url(#arrow3)"/>
        <path d="M540 110 L570 110" stroke="#FF4757" stroke-width="3" marker-end="url(#arrow3)"/>

        <!-- 전략 연결선 -->
        <path d="M100 140 L100 200" stroke="#3C40C6" stroke-width="2" stroke-dasharray="3,3"/>
        <path d="M230 140 L230 200" stroke="#5F27CD" stroke-width="2" stroke-dasharray="3,3"/>
        <path d="M360 140 L360 200" stroke="#FF4757" stroke-width="2" stroke-dasharray="3,3"/>
        <path d="M490 140 L490 200" stroke="#FFA502" stroke-width="2" stroke-dasharray="3,3"/>
        <path d="M620 140 L620 200" stroke="#2ED573" stroke-width="2" stroke-dasharray="3,3"/>

        <!-- 성과 지표 -->
        <ellipse cx="400" cy="290" rx="120" ry="25" fill="#2ED573"/>
        <text x="400" y="295" text-anchor="middle" fill="white" font-size="11" font-weight="bold">고객 생애 가치 극대화</text>

        <defs>
          <marker id="arrow3" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
            <polygon points="0 0, 10 3.5, 0 7" fill="#FF4757"/>
          </marker>
        </defs>
      </svg>
    </div>`
}