{
  "id": "ai_ml_pipeline",
  "title": "AI/ML 개발 파이프라인",
  "category": "technology",
  "keywords": ["AI", "ML", "머신러닝", "인공지능", "데이터", "모델", "딥러닝"],
  "description": "AI/ML 프로젝트의 전체 개발 파이프라인을 보여주는 기술적 프로세스 흐름도",
  "color_palette": {
    "primary": "#2E86DE",
    "secondary": "#54A0FF",
    "accent": "#5F27CD",
    "background": "#0A0A0A"
  },
  "svg_template": `
    <div class="diagram-container">
      <div class="diagram-title">{title} 핵심 프로세스</div>
      <svg width="100%" height="400" viewBox="0 0 800 400">
        <!-- 데이터 수집 -->
        <rect x="50" y="50" width="120" height="50" rx="8" fill="#2E86DE"/>
        <text x="110" y="75" text-anchor="middle" fill="white" font-size="11" font-weight="bold">데이터 수집</text>

        <!-- 데이터 전처리 -->
        <rect x="200" y="50" width="120" height="50" rx="8" fill="#54A0FF"/>
        <text x="260" y="75" text-anchor="middle" fill="white" font-size="11" font-weight="bold">데이터 전처리</text>

        <!-- 특성 엔지니어링 -->
        <rect x="350" y="50" width="120" height="50" rx="8" fill="#5F27CD"/>
        <text x="410" y="75" text-anchor="middle" fill="white" font-size="11" font-weight="bold">특성 엔지니어링</text>

        <!-- 모델 선택 -->
        <rect x="500" y="50" width="120" height="50" rx="8" fill="#3C40C6"/>
        <text x="560" y="75" text-anchor="middle" fill="white" font-size="11" font-weight="bold">모델 선택</text>

        <!-- 모델 훈련 -->
        <rect x="200" y="150" width="120" height="50" rx="8" fill="#FF6B6B"/>
        <text x="260" y="175" text-anchor="middle" fill="white" font-size="11" font-weight="bold">모델 훈련</text>

        <!-- 모델 평가 -->
        <rect x="350" y="150" width="120" height="50" rx="8" fill="#FFA502"/>
        <text x="410" y="175" text-anchor="middle" fill="white" font-size="11" font-weight="bold">모델 평가</text>

        <!-- 하이퍼파라미터 튜닝 -->
        <rect x="500" y="150" width="120" height="50" rx="8" fill="#FF4757"/>
        <text x="560" y="175" text-anchor="middle" fill="white" font-size="11" font-weight="bold">하이퍼파라미터</text>

        <!-- 모델 배포 -->
        <rect x="275" y="250" width="120" height="50" rx="8" fill="#2ED573"/>
        <text x="335" y="275" text-anchor="middle" fill="white" font-size="11" font-weight="bold">모델 배포</text>

        <!-- 모니터링 -->
        <rect x="425" y="250" width="120" height="50" rx="8" fill="#00D2D3"/>
        <text x="485" y="275" text-anchor="middle" fill="white" font-size="11" font-weight="bold">모니터링</text>

        <!-- 재훈련 -->
        <rect x="350" y="330" width="120" height="50" rx="8" fill="#6C5CE7"/>
        <text x="410" y="355" text-anchor="middle" fill="white" font-size="11" font-weight="bold">재훈련</text>

        <!-- 연결 화살표 -->
        <path d="M170 75 L200 75" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>
        <path d="M320 75 L350 75" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>
        <path d="M470 75 L500 75" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>

        <path d="M560 100 L560 120 L260 120 L260 150" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>
        <path d="M320 175 L350 175" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>
        <path d="M470 175 L500 175" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>

        <path d="M410 200 L350 230 L335 250" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>
        <path d="M395 275 L425 275" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>
        <path d="M485 300 L485 310 L410 310 L410 330" stroke="#2E86DE" stroke-width="3" marker-end="url(#arrow4)"/>

        <!-- 피드백 루프 -->
        <path d="M350 355 L50 355 L50 75" stroke="#FF4757" stroke-width="2" stroke-dasharray="5,5" marker-end="url(#arrow4)"/>

        <defs>
          <marker id="arrow4" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
            <polygon points="0 0, 10 3.5, 0 7" fill="#2E86DE"/>
          </marker>
        </defs>
      </svg>
    </div>`
}